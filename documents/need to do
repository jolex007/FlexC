need to do

operation :
	{ - add tid (void)
	} - erase tid (void)
	@ - ссылка на переменную (string)
	? - значение переменной (string)
	# - создать переменную (type, string) -> 
	## - создать функцию (type, string)
	!A - переход (number)
	!F - переход по лжи (bool, number)
	; - удаление из стека (void)
	$B - начало функции (void)
	$E - конец функции (void)
	= - присваивание (ссылку, значение) -> (новое значение)
	$F - вызов функции (парамаетры ... , название) -> (возвращаемое значение или void)

name :
	string - название чего - то

constant :
	string - число (константа)